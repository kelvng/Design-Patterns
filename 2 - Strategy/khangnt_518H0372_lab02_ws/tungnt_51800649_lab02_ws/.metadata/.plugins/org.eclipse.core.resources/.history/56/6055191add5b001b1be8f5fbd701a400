package Lab02_01.dp.tdtu;

public class InsertionSortStrategy implements ISortStrategy{
	public void sort(Comparable[] data,int count) {
		for (int j = 1; j < data.length; j++) {
			String current = data[j];
	        int i = j-1;
	        while ((i > -1) && (data[i] > current)) {
	            data[i+1] = data[i];
	            i--;
	        }
	        data[i+1] = current;
	
}
